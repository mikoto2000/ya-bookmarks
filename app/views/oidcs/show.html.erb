<p style="color: green"><%= notice %></p>

<h1><%= Oidc.model_name.human %></h1>

<%= render @oidc %>

<div>
  <%= link_to t("view.show.move_to_update", model: Oidc.model_name.human), edit_oidc_path(@oidc) %> |
  <%= link_to t("view.show.move_to_index", model: Oidc.model_name.human.pluralize), oidcs_path %>

  <%= button_to t("helpers.submit.delete"),
                @oidc,
                method: :delete,
                class: %w[btn btn-primary],
                data: { turbo_confirm: t("helpers.dialog.delete", id: @oidc.id) } %>
</div>
